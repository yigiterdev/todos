    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "react-uuid": "^1.0.2",
    "stylelint": "^14.0.1",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "uuid": "^8.3.2"
  }
