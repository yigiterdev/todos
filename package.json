    ],
    "src/**/*.{css,scss}": [
      "stylelint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hipo/eslint-config-base": "^4.1.1",
    "@hipo/eslint-config-react": "^2.1.0",
    "@hipo/eslint-config-typescript": "^1.0.1",
